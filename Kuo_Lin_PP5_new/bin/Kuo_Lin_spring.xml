<!--Yi-Chen Kuo, Chih-Wei Lin-->

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 
	<bean id="BookBeanLin" class="com.Kuo_Lin.PP5.Book">
		<property name="title" value="Chih-Wei"></property>
		<property name="author" value="Lin"></property>
	</bean>
	
	<bean id="BookBeanKuo" class="com.Kuo_Lin.PP5.Book">
		<property name="title" value="Yi-Chen"></property>
		<property name="author" value="Kuo"></property>
	</bean>
 
</beans>